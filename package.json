{
  "name": "@qingshaner/tsdoc",
  "type": "module",
  "version": "0.0.0",
  "description": "A tsdoc generator",
  "author": {
    "name": "qingshaner",
    "url": "https://github.com/Jon-a-than"
  },
  "effects": false,
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tsdoc-gen": "./bin/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "vite build",
    "changeset-version": "changeset version",
    "changeset-publish": "changeset publish",
    "format": "biome format . --write",
    "lint": "biome lint . --write && eslint . --fix",
    "prepare": "simple-git-hooks",
    "start": "vite build -m dev && node ./bin/cli.mjs",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@microsoft/api-extractor-model": "^7.30.1",
    "@microsoft/tsdoc": "^0.15.1",
    "citty": "^0.1.6",
    "consola": "^3.3.3",
    "js-yaml": "^4.1.0",
    "mdbox": "^0.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.11",
    "@qingshaner/eslint-config": "^0.8.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "lint-staged": "^15.3.0",
    "oxc-transform": "^0.44.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "pre-push": "pnpm test"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "pnpm biome lint --write",
      "pnpm eslint --cache --fix"
    ],
    "*.{js,jsx,cjs,mjs,ts,tsx,cts,mts}": "pnpm biome format --write"
  }
}
